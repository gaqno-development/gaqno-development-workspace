{
  "name": "gaqno-development-workspace",
  "version": "0.0.0",
  "private": true,
  "packageManager": "npm@11.6.2",
  "workspaces": [
    "@gaqno-backcore",
    "@gaqno-frontcore",
    "@gaqno-types",
    "gaqno-agent",
    "gaqno-admin-service",
    "gaqno-admin-ui",
    "gaqno-ai-service",
    "gaqno-ai-ui",
    "gaqno-crm-ui",
    "gaqno-erp-ui",
    "gaqno-finance-service",
    "gaqno-finance-ui",
    "gaqno-landing-ui",
    "gaqno-lenin-ui",
    "gaqno-lead-enrichment-service",
    "gaqno-omnichannel-service",
    "gaqno-omnichannel-ui",
    "gaqno-pdv-service",
    "gaqno-pdv-ui",
    "gaqno-rpg-service",
    "gaqno-rpg-ui",
    "gaqno-saas-service",
    "gaqno-saas-ui",
    "gaqno-shell-ui",
    "gaqno-sso-service",
    "gaqno-sso-ui",
    "gaqno-wellness-service",
    "gaqno-wellness-ui"
  ],
  "scripts": {
    "create-project": "node scripts/create-project.js",
    "install:all": "npm install --legacy-peer-deps",
    "clean": "rm -rf node_modules */node_modules @**/node_modules",
    "build:packages": "npm run build -w @gaqno-backcore",
    "build:types": "npm run build -w @gaqno-types",
    "build:all": "npm run build -w gaqno-shell-ui && npm run build -w gaqno-sso-ui && npm run build -w gaqno-ai-ui && npm run build -w gaqno-crm-ui && npm run build -w gaqno-erp-ui && npm run build -w gaqno-finance-ui && npm run build -w gaqno-pdv-ui && npm run build -w gaqno-rpg-ui && npm run build -w gaqno-sso-service && npm run build -w gaqno-ai-service && npm run build -w gaqno-finance-service && npm run build -w gaqno-pdv-service && npm run build -w gaqno-rpg-service",
    "dev": "turbo run dev --concurrency=20 --filter=!./gaqno-lenin-ui",
    "dev:backends": "turbo run dev --concurrency=13 --filter=./gaqno-sso-service --filter=./gaqno-ai-service --filter=./gaqno-finance-service --filter=./gaqno-pdv-service --filter=./gaqno-rpg-service",
    "dev:frontends": "turbo run dev --concurrency=13 --filter=./gaqno-shell-ui --filter=./gaqno-sso-ui --filter=./gaqno-ai-ui --filter=./gaqno-crm-ui --filter=./gaqno-erp-ui --filter=./gaqno-finance-ui --filter=./gaqno-pdv-ui --filter=./gaqno-rpg-ui",
    "dev:shell": "npm run dev -w gaqno-shell-ui",
    "dev:ai": "npm run dev -w gaqno-ai-ui",
    "dev:crm": "npm run dev -w gaqno-crm-ui",
    "dev:erp": "npm run dev -w gaqno-erp-ui",
    "dev:finance": "npm run dev -w gaqno-finance-ui",
    "dev:pdv": "npm run dev -w gaqno-pdv-ui",
    "dev:sso": "npm run dev -w gaqno-sso-ui",
    "dev:sso-service": "npm run start:dev -w gaqno-sso-service",
    "dev:ai-service": "npm run start:dev -w gaqno-ai-service",
    "dev:finance-service": "npm run start:dev -w gaqno-finance-service",
    "dev:pdv-service": "npm run start:dev -w gaqno-pdv-service",
    "dev:rpg-service": "npm run start:dev -w gaqno-rpg-service",
    "dev:wellness": "npm run dev -w gaqno-wellness-ui",
    "dev:wellness-service": "npm run start:dev -w gaqno-wellness-service",
    "release:packages": "./publish-packages.sh",
    "publish:frontcore": "sh -c 'set -a && [ -f .env ] && . ./.env && set +a && npm publish -w @gaqno-development/frontcore'",
    "publish:confluence": "echo 'Docs no Confluence DDS. Edite em https://gaqno-development.atlassian.net/wiki/spaces/DDS'"
  },
  "devDependencies": {
    "inquirer": "^9.3.8",
    "turbo": "^2.3.0"
  },
  "dependencies": {
    "rxjs": "^7.8.2"
  }
}
