  "name": "@gaqno-development/backcore",
  "version": "1.1.13",
  "private": false,
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/gaqno-development",
    "access": "restricted"
  },
  "files": ["dist"],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "services/*": [
        "./dist/services/*"
      ],
      "modules/*": [
        "./dist/modules/*"
      ],
      "schemas/*": [
        "./dist/schemas/*"
      ],
      "config/*": [
        "./dist/config/*"
      ],
      "middleware/*": [
        "./dist/middleware/*"
      ]
    }
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "jest --passWithNoTests"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "rootDir": ".",
    "testMatch": ["**/__tests__/**/*.spec.ts"],
    "moduleFileExtensions": ["ts", "js"]
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./services": {
      "require": "./dist/services/index.js",
      "import": "./dist/services/index.js",
      "types": "./dist/services/index.d.ts"
    },
    "./services/base-crud.service": {
      "require": "./dist/services/base-crud.service.js",
      "import": "./dist/services/base-crud.service.js",
      "types": "./dist/services/base-crud.service.d.ts"
    },
    "./services/base-crud.controller": {
      "require": "./dist/services/base-crud.controller.js",
      "import": "./dist/services/base-crud.controller.js",
      "types": "./dist/services/base-crud.controller.d.ts"
    },
    "./services/sso.service": {
      "require": "./dist/services/sso.service.js",
      "import": "./dist/services/sso.service.js",
      "types": "./dist/services/sso.service.d.ts"
    },
    "./modules": {
      "require": "./dist/modules/index.js",
      "import": "./dist/modules/index.js",
      "types": "./dist/modules/index.d.ts"
    },
    "./modules/*": {
      "require": "./dist/modules/*.js",
      "import": "./dist/modules/*.js",
      "types": "./dist/modules/*.d.ts"
    },
    "./schemas": {
      "require": "./dist/schemas/index.js",
      "import": "./dist/schemas/index.js",
      "types": "./dist/schemas/index.d.ts"
    },
    "./schemas/*": {
      "require": "./dist/schemas/*.js",
      "import": "./dist/schemas/*.js",
      "types": "./dist/schemas/*.d.ts"
    },
    "./config": {
      "require": "./dist/config/index.js",
      "import": "./dist/config/index.js",
      "types": "./dist/config/index.d.ts"
    },
    "./config/env.validation": {
      "require": "./dist/config/env.validation.js",
      "import": "./dist/config/env.validation.js",
      "types": "./dist/config/env.validation.d.ts"
    },
    "./config/*": {
      "require": "./dist/config/*.js",
      "import": "./dist/config/*.js",
      "types": "./dist/config/*.d.ts"
    },
    "./middleware": {
      "require": "./dist/middleware/index.js",
      "import": "./dist/middleware/index.js",
      "types": "./dist/middleware/index.d.ts"
    },
    "./middleware/*": {
      "require": "./dist/middleware/*.js",
      "import": "./dist/middleware/*.js",
      "types": "./dist/middleware/*.d.ts"
    }
  },
  "peerDependencies": {
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/throttler": "^6.3.0",
    "axios": "^1.6.8",
    "class-validator": "^0.14.1",
    "express": "^4.18.2",
    "typescript": "^5.0.0",
    "zod": "^3.0.0"
  },
  "dependencies": {
    "@gaqno-development/types": "^1.0.0",
    "axios": "^1.6.8",
    "drizzle-orm": "^0.30.0",
    "kafkajs": "^2.2.4",
    "pg": "^8.11.0",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@nestjs/axios": "^3.1.3",
    "@nestjs/common": "^10.4.20",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.20",
    "@nestjs/throttler": "^6.5.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.0",
    "class-validator": "^0.14.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.0.0"
  }
}
