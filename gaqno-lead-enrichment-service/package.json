{
  "name": "gaqno-lead-enrichment-service",
  "version": "0.0.1",
  "description": "Kafka consumer for lead enrichment via Pipedrive API",
  "scripts": {
    "start": "node dist/main.js",
    "dev": "nest start --watch",
    "build": "nest build",
    "lint": "eslint \"src/**/*.ts\"",
    "migrate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "test": "jest",
