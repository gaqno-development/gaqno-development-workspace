    "test:pipedrive": "nest build && node dist/scripts/pipedrive-search-manual.js"
  },
  "dependencies": {
    "@gaqno-development/backcore": "^1.1.15",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/throttler": "^6.3.0",
    "axios": "^1.6.0",
    "drizzle-orm": "^0.30.10",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/testing": "^10.4.22",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.11.19",
    "@types/pg": "^8.10.9",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.3.3",
    "drizzle-kit": "^0.29.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
